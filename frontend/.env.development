# Solana Network Settings
REACT_APP_SOLANA_NETWORK=devnet
REACT_APP_RPC_ENDPOINT=https://api.devnet.solana.com
REACT_APP_WEBSOCKET_ENDPOINT=wss://api.devnet.solana.com

# Token Configuration
REACT_APP_TOKEN_ADDRESS=your_token_address_here
REACT_APP_TOKEN_DECIMALS=9
REACT_APP_TOKEN_SYMBOL=TOKEN

# API Keys and External Services
REACT_APP_COINGECKO_API=https://api.coingecko.com/api/v3
REACT_APP_PRICE_FEED_API=your_price_feed_api_here

# Feature Flags
REACT_APP_ENABLE_TESTNET=true
REACT_APP_ENABLE_MAINNET=false
REACT_APP_ENABLE_SWAP=true

# Cache Settings
REACT_APP_CACHE_DURATION=300
REACT_APP_PRICE_UPDATE_INTERVAL=30

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_VERBOSE_LOGGING=true

